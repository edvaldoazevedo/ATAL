package pilha;

public class Pilha {
	static final int MAX = 50;
	private int n;
	private int vetor[] = new int [MAX];
	private Pilha p;
	
	
	public Pilha criaPilha(){
		p = new Pilha();
		p.n = 0;
		
		return p;
	}
	
	public void push(Pilha p, int elemento ){
		if(p.n == Pilha.MAX){
			System.out.println("Estouro de Pilha!");
		}
	}

}
