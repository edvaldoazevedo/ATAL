package pilha;

public class Pilha {
	
	private int n;
	private int vetor[];
	private int size;
	
	
	public Pilha criaPilha(int capacidade){
		vetor = new int[capacidade];
		size= 0;
		
	}
	
	public void push(Pilha p, int elemento ){
		if(p.n == Pilha.MAX){
			System.out.println("Estouro de Pilha!");
		}
		p.vetor[p.n] = elemento;
		p.n++;
	}
	

}
