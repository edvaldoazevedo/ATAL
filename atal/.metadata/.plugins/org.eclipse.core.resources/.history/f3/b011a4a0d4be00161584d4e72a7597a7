package pilha;

public class Pilha {
	
	private int topo = 0;
	private int vetor[];
	private int size;
	
	
	public Pilha(int capacidade){
		vetor = new int[capacidade];
		topo= 0;
		
	}
	
	public int push(int elemento ){
		if(topo == vetor.length){
			return 0;
		}
		vetor[topo] = elemento;
		topo++;
		return elemento;
	}
	
	public int peek(){
	 int elemento = vetor[topo - 1]; 
	 return elemento;
	}
	public int pop(){
		int removido = vetor[topo-1];
		vetor[topo-1] = null;
		
	}
	

}
