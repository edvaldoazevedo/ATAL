package pilha;

public class Pilha {
	
	private int topo = 0;
	private int vetor[];
	private int size;
	
	
	public Pilha(int capacidade){
		vetor = new int[capacidade];
		size= 0;
		
	}
	
	public Object push(int elemento ){
		if(size == vetor.length){
			return null;
		}
		
		vetor[topo] = elemento;
		size++;
		
		
	}
	

}
