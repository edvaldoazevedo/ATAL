package fila;

public class Fila {

	private int inicio = 0;
	private int fim = 0;
	private int fila[];

	public Fila(int tam) {
		fila = new int[tam];
	}
	public void enfileirar(int elemento) {
		
		fila[fim] = elemento;
		if (fila[fim] == fila.length) {
			fila[fim] = 1;
		} else {
			fila[fim] = fila[fim] + 1;
		}

	}

	public int desinfileirar() {
		int elemento = fila[inicio];
		if (fila[inicio] == fila.length) {
			fila[inicio] = 1;
		}else{
			fila[inicio] = fila[inicio] + 1;
		}
		return elemento;
	}

}
