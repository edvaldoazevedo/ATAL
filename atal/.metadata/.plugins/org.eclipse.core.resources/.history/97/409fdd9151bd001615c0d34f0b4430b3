package fila;

public class Fila {
	
	private int inicio = 0;
	private int fim = 0;
	private int fila[];
	
	
	public void enfileirar(int elemento){
		if(inicio ==  fim){
			
			fila[inicio] = elemento;
			fila[fim] = inicio + 1 ;
		}else{
			fila[fim + 1] = elemento;
		}
		
	}
	
	public void desinfileirar(){
		fila[inicio] = fila; 
	}

}
